{
  "compilerOptions": {
    "target": "ES6" /* Or "ES2015", or later */,
    "module": "ES6",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES2015", "dom"],
    "moduleResolution": "node"
  },
  "include": ["./src/**/*.ts"],
  "exclude": ["node_modules"]
}
